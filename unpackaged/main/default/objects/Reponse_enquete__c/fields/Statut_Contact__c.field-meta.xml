<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Statut_Contact__c</fullName>
    <description>En fonction du score NPS, détermine le statut : Promoteur, Passif ou Détracteur</description>
    <externalId>false</externalId>
    <formula>IF(Score_NPS__c &gt; 8, $Label.Qualtrics_PROMOTEUR2,  IF(Score_NPS__c &gt; 6 &amp;&amp; Score_NPS__c &lt; 9, $Label.Qualtrics_PASSIVE,  IF(Score_NPS__c &lt; 7, $Label.Qualtrics_DETRACTEUR, &quot;&quot;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Statut Contact</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
