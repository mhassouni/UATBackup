<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_After_Pickup_Date__c</fullName>
    <description>Champ fait pour calculer si la Date début de location a dépassé 6 semaine.
Besoin : la checkbox &quot;Réservation à confirmer&quot;.</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL( Pick_up_Date__c ),50, Pick_up_Date__c - NOW() )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days After Pickup Date</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
