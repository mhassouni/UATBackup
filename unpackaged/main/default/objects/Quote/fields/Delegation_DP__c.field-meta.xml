<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Delegation_DP__c</fullName>
    <description>Calcul délégation DP à partir du loyer country manager et loyer devis</description>
    <formula>IF
(
NOT(ISBLANK(  Loyer_DO_DF__c ))
,
(( Loyer__c -  Loyer_DO_DF__c ))/Loyer__c
,
0
)</formula>
    <label>Délégation DP</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
