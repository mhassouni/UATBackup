<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Delegation_DS__c</fullName>
    <description>Calcul délégation DS à partir du loyer devis et loyer depot manager</description>
    <formula>IF
(
NOT(ISBLANK( Loyer_Directeur_de_site__c ))
,
(( Loyer__c -  Loyer_Directeur_de_site__c )/Loyer__c)
,
0
)</formula>
    <label>Délégation DS</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
