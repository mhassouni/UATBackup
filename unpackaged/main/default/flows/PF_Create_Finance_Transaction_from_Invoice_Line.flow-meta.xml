    <decisions>
        <description>Is it an internal document? For internal documents, we do not generate Finance Transactions.</description>
        <name>Is_Internal</name>
        <label>Is Internal?</label>
        <locationX>176</locationX>
        <locationY>335</locationY>
        <defaultConnector>
            <targetReference>Event</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>IsInternalYes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.blng__Invoice__r.PF_Internal__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>when tax is 20% set to TN
when tax is 0% set to EXO</description>
        <name>Tax_Percentage</name>
        <label>Tax Percentage?</label>
        <locationX>275</locationX>
        <locationY>695</locationY>
        <defaultConnector>
            <targetReference>Set_EXO</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Otherwise assume 0</defaultConnectorLabel>
        <rules>
            <name>X20</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.blng__TaxPercentageApplied__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_TN</targetReference>
            </connector>
            <label>20</label>
        </rules>
    </decisions>
