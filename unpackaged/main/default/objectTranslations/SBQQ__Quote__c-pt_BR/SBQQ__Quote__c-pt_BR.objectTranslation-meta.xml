    <validationRules>
        <errorMessage><!-- Your quote is currently under calculation. Please try in a few seconds. --></errorMessage>
        <name>Block_change_status_during_recalculation</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Ce devis est &quot;Courte Durée&quot;. Vous n&apos;êtes pas autorisé à modifier les valeurs pour la caution ou le dépot de garantie. --></errorMessage>
        <name>No_Caution_dpt_de_garantie_if_CD</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Les prix ont été définis, l&apos;édition du devis est désormais bloquée. Vous ne pouvez plus modifier les options et les quantités. --></errorMessage>
        <name>No_Option_Deleted</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You cannot create more than one Information Sheet for this Quote. --></errorMessage>
        <name>Prevent_Creation_of_Several_IS</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Les groupes de ligne de devis ne peuvent pas avoir la même date de fin.
Si c&apos;est le cas, merci de créer un devis pour chaque groupe de ligne de devis.
Cela permettra la création d&apos;un contrat unique pour chaque groupe. --></errorMessage>
        <name>QLG_Similar_End_Date</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The End Date must come after the Start Date. --></errorMessage>
        <name>SBQQ__End_Date_Before_Start_Date</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- First Segment Term End Date must fall between Quote Start Date and Quote End Date --></errorMessage>
        <name>SBQQ__Invalid_First_Segment_Term_End_Date</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The subscription term must be a positive number. --></errorMessage>
        <name>SBQQ__Invalid_Subscription_Term</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Votre devis nécessite une approbation. Merci de cliquer sur le bouton &quot;Soumettre pour approbation&quot; --></errorMessage>
        <name>VR_Approval_Process</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Les champs &quot; Condition contractuelle&quot; sont obligatoires --></errorMessage>
        <name>VR_ConditionsContractuelle_NotBlank</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous devez renseigner le champ &quot;SI autres précisez&quot; quand vous renseignez &quot;Autres&quot; dans &quot;Mois de caution&quot; --></errorMessage>
        <name>VR_MoisCaution</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Cliquer sur le bouton “Finaliser Devis” pour créer le contrat. --></errorMessage>
        <name>VR_StatusSigned_FinaliserDevis</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous n&apos;êtes pas autorisé à passer ce devis à &quot;En attente de remise&quot; ou &quot;Remise Accordée&quot; manuellement. --></errorMessage>
        <name>VR_Status_En_Attente_de_Remise</name>
    </validationRules>
