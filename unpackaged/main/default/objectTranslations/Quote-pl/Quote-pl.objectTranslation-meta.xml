    <validationRules>
        <errorMessage><!-- Vous ne pouvez pas envoyer de demande de tarification si le nombre de Km, la durée de location ne sont pas renseignés et un service tarification choisi. --></errorMessage>
        <name>RV01_Conditions_Demande_tarification</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous n&apos;étes pas autorisé à modifier le statut du devis --></errorMessage>
        <name>RV03_Status_profil_tarification</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous ne pouvez pas faire une demande de tarification si votre opportunité n&apos;est pas en statut 2,3 ou 4 --></errorMessage>
        <name>RV04_DemandeTarification_StatutOpp</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Renseigner tous les champs [Type de produit / Sous-Famille / Catégorie / Sous-Catégorie / Quantité / Durée / KM / Loyer / Prix du km supplémentaire les 100kms ] avant de synchroniser le devis.Le compte de votre opportunité/devis doit être validé. --></errorMessage>
        <name>RV05_Conditions_Sync_Devis_Opportunity</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous devez renseigner Cloison(Position) quand Cloison(Type) est renseigné --></errorMessage>
        <name>RV07_PositionCloison_Obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous devez renseigner le &quot;Crochet par rail à viande (Qté)&quot; et &quot;Penderie  / Porte cintre (Qté)
&quot; si &quot;Penderie / Porte cintre (Type)&quot; est renseigné --></errorMessage>
        <name>RV08_RailViande_Obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous devez renseigner &quot;Etagère (Qté par face)&quot; et &quot;Etagère (Hauteur / plancher)&quot; si &quot;Etagère (Type)&quot; est renseigné. --></errorMessage>
        <name>RV09_QteEtageres_Obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous devez renseigner la &quot;Arrimage(s) sup.(Hauteur / plancher)&quot; si &quot;Arrimage(s) sup.(Qté par face)&quot;	 est renseigné --></errorMessage>
        <name>RV10_NiveauRailArrimage_Obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous ne pouvez pas renseigner un loyer inférieur au loyer proposé par la tarification.Merci de faire une dérogation. --></errorMessage>
        <name>RV11_LoyerDepotManager</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Assurez vous d&apos;avoir un loyer avant de saisir le Loyer Depot Manager ou Loyer Country Manager --></errorMessage>
        <name>RV_18_Delegation_Tarification</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Hors délégation.Merci de créer une dérogation. --></errorMessage>
        <name>VR12_LoyerCountryManager</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous ne pouvez  pas renseigner un loyer inférieur au loyer proposé par la tarification.Merci de faire une dérogation. --></errorMessage>
        <name>VR14_Sansdelegation_DepotManager</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous ne pouvez pas renseigner un loyer inférieur au loyer proposé par la tarification.Merci de faire une demande de dérogation --></errorMessage>
        <name>VR15_SansDelegation_CountryManager</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Vous devez renseigner &quot;Ouverture latérale (Position)&quot;,&quot;Ouverture latérale (Qté)&quot; et &quot;Largeur utile ouverture latérale (mm)&quot; si &quot;Ouverture latérale (Type)&quot; est renseigné --></errorMessage>
        <name>VR17_PositionPorteLateral_Obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Veuillez renseigner &quot;Barre étais (Qté)&quot; si &quot;Barre étai (Type)&quot; est renseigné --></errorMessage>
        <name>VR18_BarreEtaisQte_Obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Le compte doit être validé pour pouvoir synchroniser le devis. Merci de contacter votre crédit manager. --></errorMessage>
        <name>VR20_Sycnhroniser_CompteValide</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Si le champ Etagère (Qté par face) est renseigné, vous devez renseigner &quot;Etagère (Type)&apos; et &quot;Etagère (Hauteur / plancher)&quot; --></errorMessage>
        <name>VR_Etageres</name>
    </validationRules>
